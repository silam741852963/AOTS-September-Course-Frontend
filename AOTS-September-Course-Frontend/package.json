{
  "name": "aots-september-course-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently --kill-others \"vite\" \"npm run sass\"",
    "build": "concurrently --kill-others \"vite build\" \"sass ./src/scss/style.scss ./src/style.css\"",
    "preview": "vite preview",
    "sass": "sass --watch ./src/scss/style.scss ./src/style.css"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "concurrently": "^8.2.1",
    "sass": "^1.67.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "autoprefixer": "^10.4.15",
    "postcss": "^8.4.30",
    "tailwindcss": "^3.3.3",
    "vite": "^4.4.5"
  }
}
